{
  "configs": {
    "monthlySavePath": "./downloads/monthly",
    "DailySavePath": "./downloads/daily"
  },

  "monthly": [
    {
      "name": "xd",
      "host": "ftp.test.xd",
      "user": "user",
      "pass": "pass",
      "secure": false,
      "fileDir": "/logs/",
      "fileGeneration": {
        "yearDigits": 2,
        "monthList": [
          "Jan",
          "Feb",
          "Mar",
          "Apr",
          "May",
          "Jun",
          "Jul",
          "Aug",
          "Sep",
          "Oct",
          "Nov",
          "Dec"
        ],
        "format": "ftp.test.xd_${m}-${y}",
        "ext": ".gz"
      }
    }
  ],

  "daily": [
    {
      "name": "xd2",
      "host": "ftp.test.xd2",
      "user": "user",
      "pass": "pass",
      "secure": false,
      "fileDir": "/logs",
      "fileGeneration": {
        "dayMonthDigits": "2",
        "yearDigits": 2,
        "format": "access.log.${d}-${m}-${y}",
        "ext": ".gz"
      }
    }
  ]
}
